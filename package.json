{
    "name": "@peacockproject/statemachine-parser",
    "version": "3.1.0",
    "description": "IOI state machine conditional parser.",
    "main": "build/cjs/index.js",
    "module": "build/esm/index.js",
    "types": "build/index.d.ts",
    "homepage": "https://thepeacockproject.org",
    "sideEffects": false,
    "engines": {
        "node": ">=14"
    },
    "directories": {
        "test": "tests",
        "lib": "build"
    },
    "files": [
        "README.md",
        "build/cjs/*",
        "build/esm/*",
        "LICENSE",
        "src"
    ],
    "keywords": [
        "ioi",
        "state",
        "machines",
        "glacier",
        "modding"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/thepeacockproject/StateMachineParser"
    },
    "author": "The Peacock Project <admin@thepeacockproject.org> (https://thepeacockproject.org)",
    "license": "Apache-2.0",
    "packageManager": "yarn@3.2.0",
    "exports": {
        "import": "./build/esm/index.js",
        "require": "./build/cjs/index.js"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.23",
        "mocha": "^9.2.2",
        "prettier": "^2.6.2",
        "ts-node": "^10.7.0",
        "typescript": "4.6.3"
    },
    "scripts": {
        "prettier": "prettier --write \"**/*.{ts,md,json,js,cjs}\"",
        "build": "tsc && tsc -p tsconfig-esm.json",
        "test": "mocha --require ts-node/register --extension js,ts,cjs,mjs tests"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4
    },
    "publishConfig": {
        "access": "public"
    }
}
